// Workspace create → work → merge → cleanup lifecycle
digraph WorkspaceLifecycle {
  graph [goal="Test workspace lifecycle"]
  start      [shape=Mdiamond]
  exit       [shape=Msquare]
  ws_create  [type="workspace.create", workspace_name="integ-test"]
  work       [shape=box, prompt="Do work in workspace"]
  ws_merge   [type="workspace.merge"]
  ws_cleanup [type="workspace.cleanup"]

  start -> ws_create -> work -> ws_merge -> ws_cleanup -> exit
}
